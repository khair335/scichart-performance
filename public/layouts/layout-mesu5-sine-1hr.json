{
  "layout_mode": "multi_surface",
  "grid": [3, 1],
  "xAxis": {
    "defaultRange": {
      "mode": "lastMinutes",
      "value": 2
    }
  },
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "yField": "y"
    }
  },
  "panes": [
    {
      "id": "price-pane",
      "row": 0,
      "col": 0,
      "height": 2,
      "width": 1,
      "title": "MESU5 Sine Wave (1hr period) - Ticks + 9 SMAs"
    },
    {
      "id": "ohlc-pane",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "OHLC Bars (10s & 30s)"
    },
    {
      "id": "pnl-pane",
      "row": 2,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "Strategy PnL"
    }
  ],
  "series": [
    {
      "series_id": "MESU5:ticks",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#50C7E0",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_10",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#F48420",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_20",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#26A69A",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_30",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#AB47BC",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_40",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#FF7043",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_50",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#7E57C2",
        "strokeThickness": 2
      }
    },
    {
      "series_id": "MESU5:sma_60",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#66BB6A",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_70",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#42A5F5",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_80",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#EC407A",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:sma_90",
      "pane": "price-pane",
      "type": "FastLineRenderableSeries",
      "style": {
        "stroke": "#FFCA28",
        "strokeThickness": 1
      }
    },
    {
      "series_id": "MESU5:ohlc_time:10000",
      "pane": "ohlc-pane",
      "type": "FastCandlestickRenderableSeries",
      "style": {
        "upWickColor": "#26A69A",
        "downWickColor": "#EF5350",
        "upBodyColor": "#26A69A",
        "downBodyColor": "#EF5350"
      }
    },
    {
      "series_id": "MESU5:ohlc_time:30000",
      "pane": "ohlc-pane",
      "type": "FastCandlestickRenderableSeries",
      "style": {
        "upWickColor": "#42A5F5",
        "downWickColor": "#FF7043",
        "upBodyColor": "#42A5F5",
        "downBodyColor": "#FF7043"
      }
    },
    {
      "series_id": "MESU5:strategy:alpha:pnl",
      "pane": "pnl-pane",
      "type": "FastMountainRenderableSeries",
      "style": {
        "stroke": "#4CAF50",
        "strokeThickness": 2,
        "fill": "rgba(76, 175, 80, 0.3)"
      }
    }
  ],
  "strategy_markers": {
    "exclude_panes": ["pnl-pane"]
  },
  "meta": {
    "version": "1.0",
    "name": "MESU5 Sine Wave 1-Hour Period Test",
    "description": "40 Hz ticks with sine wave price model (3600s period), 9 SMAs, OHLC, PnL - 12M samples"
  }
}
