{
  "layout_mode": "multi_surface",
  "grid": [2, 2],
  "xAxis": {
    "defaultRange": {
      "mode": "lastMinutes",
      "value": 2
    }
  },
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "yField": "y"
    }
  },
  "panes": [
    {
      "id": "tick-pane",
      "row": 0,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "Tick Price"
    },
    {
      "id": "indicator-pane",
      "row": 0,
      "col": 1,
      "height": 1,
      "width": 1,
      "title": "Indicators"
    },
    {
      "id": "ohlc-pane",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "OHLC Bars"
    },
    {
      "id": "pnl-pane",
      "row": 1,
      "col": 1,
      "height": 1,
      "width": 1,
      "title": "Strategy PnL"
    }
  ],
  "series": [
    {
      "series_id": "MESU5:ticks",
      "pane": "tick-pane",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_10",
      "pane": "indicator-pane",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:ohlc_time:10000",
      "pane": "ohlc-pane",
      "type": "FastCandlestickRenderableSeries"
    },
    {
      "series_id": "MESU5:strategy:alpha:pnl",
      "pane": "pnl-pane",
      "type": "FastLineRenderableSeries"
    }
  ],

  "meta": {
    "version": "1.0",
    "description": "Layout testing strategy markers - should appear only on tick-pane and indicator-pane (explicitly included)"
  }
}
