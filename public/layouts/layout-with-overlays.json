{
  "layout_mode": "multi_surface",
  "grid": [2, 2],
  "xAxis": {
    "defaultRange": {
      "mode": "lastMinutes",
      "value": 2
    }
  },
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "yField": "y"
    }
  },
  "panes": [
    {
      "id": "topLeft",
      "row": 0,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "RSI Indicator",
      "overlays": {
        "hline": [
          {
            "id": "rsi30",
            "y": 30,
            "label": "30",
            "style": {
              "stroke": "#FF6B6B",
              "strokeThickness": 1,
              "strokeDashArray": [5, 5]
            }
          },
          {
            "id": "rsi70",
            "y": 70,
            "label": "70",
            "style": {
              "stroke": "#4ECDC4",
              "strokeThickness": 1,
              "strokeDashArray": [5, 5]
            }
          },
          {
            "id": "rsi50",
            "y": 50,
            "label": "50",
            "style": {
              "stroke": "#95A5A6",
              "strokeThickness": 1,
              "strokeDashArray": [2, 2]
            }
          }
        ]
      }
    },
    {
      "id": "topRight",
      "row": 0,
      "col": 1,
      "height": 1,
      "width": 1,
      "title": "Price with Support/Resistance",
      "overlays": {
        "hline": [
          {
            "id": "support",
            "y": 4500,
            "label": "Support",
            "style": {
              "stroke": "#2ECC71",
              "strokeThickness": 2
            }
          },
          {
            "id": "resistance",
            "y": 4600,
            "label": "Resistance",
            "style": {
              "stroke": "#E74C3C",
              "strokeThickness": 2
            }
          }
        ]
      }
    },
    {
      "id": "bottomLeft",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "OHLC with Zero Line",
      "overlays": {
        "hline": [
          {
            "id": "zero",
            "y": 0,
            "label": "0",
            "style": {
              "stroke": "#95A5A6",
              "strokeThickness": 1,
              "strokeDashArray": [3, 3]
            }
          }
        ]
      }
    },
    {
      "id": "bottomRight",
      "row": 1,
      "col": 1,
      "height": 1,
      "width": 1,
      "title": "Volume Profile"
    }
  ],
  "series": [
    {
      "series_id": "MESU5:ticks",
      "pane": "topRight",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_10",
      "pane": "topRight",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:ohlc_time:10000",
      "pane": "bottomLeft",
      "type": "FastCandlestickRenderableSeries"
    }
  ],
  "strategy_markers": {
    "exclude_panes": ["bottomLeft"]
  },
  "meta": {
    "version": "1.0",
    "description": "Layout with horizontal line overlays (RSI levels, support/resistance, zero line)"
  }
}
