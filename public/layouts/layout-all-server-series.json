        "hlines": [
          {
            "id": "support",
            "y": 4500,
            "label": "Support",
            "style": {
              "stroke": "#2ECC71",
              "strokeThickness": 2,
              "strokeDashArray": [5, 5]
            }
          },
          {
            "id": "resistance",
            "y": 4600,
            "label": "Resistance",
            "style": {
              "stroke": "#E74C3C",
              "strokeThickness": 2,
              "strokeDashArray": [5, 5]
