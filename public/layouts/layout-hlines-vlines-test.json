        "hline": [
          {
            "id": "support_1",
            "y": 2999,
            "label": "Support Level",
            "style": {
              "stroke": "#00FF00",
              "strokeThickness": 2,
              "strokeDashArray": [5, 5]
            }
          },
          {
            "id": "resistance_1",
            "y": 3001,
            "label": "Resistance Level",
            "style": {
              "stroke": "#FF0000",
              "strokeThickness": 2,
              "strokeDashArray": [5, 5]
            }
          },
          {
            "id": "pivot",
            "y": 3000,
            "label": "Pivot Point",
            "style": {
              "stroke": "#FFFF00",
              "strokeThickness": 1,
              "strokeDashArray": [2, 2]
            }
          }
        ],
        "vline": [
          {
            "id": "session_start",
            "x": 1766217600000,
            "label": "08:00 AM Marker",
            "style": {
              "stroke": "#00BFFF",
