            "style": {
              "stroke": "#FF6600",
              "strokeThickness": 2
            }
          },
          {
            "id": "daily_low",
            "y": 2998.5,
            "label": "Daily Low",
            "style": {
              "stroke": "#6600FF",
              "strokeThickness": 2
            }
          }
        ]
      }
    }
