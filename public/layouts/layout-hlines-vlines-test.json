{
  "layout_mode": "multi_surface",
  "grid": [2, 1],
  "min_height": 800,
  "xAxis": {
    "defaultRange": {
      "mode": "session"
    }
  },
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "pane": "price"
    }
  },
  "panes": [
    {
