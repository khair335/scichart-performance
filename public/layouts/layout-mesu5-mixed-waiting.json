    },
    {
      "series_id": "MESU5:strategy:alpha:signals",
      "pane": "bottomRight",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:strategy:beta:signals",
      "pane": "bottomRight",
      "type": "FastLineRenderableSeries"
    }
  ],
  "strategy_markers": {
