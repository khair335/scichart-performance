    {
      "id": "topRight",
      "row": 0,
      "col": 1,
      "height": 1,
      "width": 1,
      "title": "MESU5 Indicators (Mixed)"
    },
    {
      "id": "bottomLeft",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "MESU5 OHLC 10s (Fast)"
