{
  "layout_mode": "multi_surface",
  "grid": [2, 1],
  "panes": [
    {
      "id": "tick-pane",
      "title": "Tick Price & Indicators",
      "row": 0,
      "col": 0,
      "height": 1,
      "width": 1
    },
    {
      "id": "ohlc-pane",
      "title": "OHLC Candlesticks",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "isBar": true
    }
  ],
  "series": [
    {
      "series_id": "MESU5:ticks",
      "pane": "tick-pane",
      "type": "FastLineRenderableSeries",
      "color": "#50C7E0",
      "strokeThickness": 1
    },
    {
      "series_id": "MESU5:sma_10",
      "pane": "tick-pane",
      "type": "FastLineRenderableSeries",
      "color": "#F48420",
      "strokeThickness": 1
    },
    {
      "series_id": "MESU5:ohlc_time:3600000",
      "pane": "ohlc-pane",
      "type": "FastCandlestickRenderableSeries"
    }
  ],
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "yField": "y"
    }
  },
  "meta": {
    "version": "1.0",
    "name": "Default 2x1 Layout",
    "description": "Simple 2-row layout with tick price and OHLC candlesticks"
  }
}
