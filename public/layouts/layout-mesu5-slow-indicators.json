{
  "layout_mode": "multi_surface",
  "grid": [3, 1],
  "xAxis": {
    "defaultRange": {
      "mode": "lastMinutes",
      "value": 2
    }
  },
  "minimap": {
    "source": {
      "series_id": "MESU5:ticks",
      "yField": "y"
    }
  },
  "panes": [
    {
      "id": "pane1",
      "row": 0,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "MESU5 Ticks (Fast)"
    },
    {
      "id": "pane2",
      "row": 1,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "MESU5 Long Window Indicators (Slow - Will Wait)"
    },
    {
      "id": "pane3",
      "row": 2,
      "col": 0,
      "height": 1,
      "width": 1,
      "title": "MESU5 60min OHLC (Very Slow - Will Wait)"
    }
  ],
  "series": [
    {
      "series_id": "MESU5:ticks",
      "pane": "pane1",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_60",
      "pane": "pane2",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_70",
      "pane": "pane2",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_80",
      "pane": "pane2",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:sma_90",
      "pane": "pane2",
      "type": "FastLineRenderableSeries"
    },
    {
      "series_id": "MESU5:ohlc_time:3600000",
      "pane": "pane3",
      "type": "FastCandlestickRenderableSeries"
    }
  ],
  "strategy_markers": {
    "exclude_panes": ["pane3"]
  },
  "meta": {
    "version": "1.0",
    "name": "MESU5 Slow Indicators Test"
  }
}

